    n = int(input())
    for i in range(n):
        print(i+1, end= "")

-------------------------------------------------------
List Comprehensions
------------------------------------------------------
 if __name__ == '__main__':
    x = int(input())
    y = int(input())
    z = int(input())
    n = int(input())
    main=list()
    main=[[i,j,k] for i in range(x+1) for j in range(y+1) for k in range(z+1) if (i+j+k)!=n]
    print(main)
    
    
